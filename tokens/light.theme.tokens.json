{
    "$theme": "light",
    "grid": {
        "mobile": {
            "columns": {
                "$type": "number",
                "$value": 4
            },
            "gutter": {
                "$type": "string",
                "$value": "{spacing.1}"
            },
            "margin": {
                "$type": "string",
                "$value": "{spacing.4}"
            }
        },
        "tablet": {
            "columns": {
                "$type": "number",
                "$value": 8
            },
            "gutter": {
                "$type": "string",
                "$value": "{spacing.3}"
            },
            "margin": {
                "$type": "string",
                "$value": "{spacing.8}"
            }
        },
        "desktop": {
            "columns": {
                "$type": "number",
                "$value": 12
            },
            "gutter": {
                "$type": "string",
                "$value": "{spacing.5}"
            },
            "margin": {
                "$type": "string",
                "$value": "auto"
            }
        }
    },
    "responsive": {
        "{breakpoint}": {
            "$type": "string",
            "$value": "{$value}"
        }
    },
    "font-size": {
        "extra-large": {
            "$type": "string",
            "$value": "{font-size.12}"
        },
        "large": {
            "$type": "string",
            "$value": "{font-size.8}"
        },
        "medium": {
            "$type": "string",
            "$value": "{font-size.4}"
        },
        "small": {
            "$type": "string",
            "$value": "{font-size.3}"
        },
        "extra-small": {
            "$type": "string",
            "$value": "{font-size.1}"
        }
    },
    "font-weight": {
        "thin": {
            "$type": "number",
            "$value": 100
        },
        "extra-light": {
            "$type": "number",
            "$value": 200
        },
        "light": {
            "$type": "number",
            "$value": 300
        },
        "normal": {
            "$type": "number",
            "$value": 400
        },
        "medium": {
            "$type": "number",
            "$value": 500
        },
        "semi-bold": {
            "$type": "number",
            "$value": 600
        },
        "bold": {
            "$type": "number",
            "$value": 700
        },
        "extra-bold": {
            "$type": "number",
            "$value": 800
        },
        "black": {
            "$type": "number",
            "$value": 900
        }
    },
    "line-height": {
        "extra-large": {
            "$type": "string",
            "$value": "{line-height.12}"
        },
        "large": {
            "$type": "string",
            "$value": "{line-height.8}"
        },
        "medium": {
            "$type": "string",
            "$value": "{line-height.4}"
        },
        "small": {
            "$type": "string",
            "$value": "{line-height.3}"
        },
        "extra-small": {
            "$type": "string",
            "$value": "{line-height.1}"
        }
    },
    "color": {
        "primary": {
            "default": {
                "$type": "color",
                "$value": "#6750A4"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#EADDFF"
            },
            "on-container": {
                "$type": "color",
                "$value": "#21005D"
            }
        },
        "secondary": {
            "default": {
                "$type": "color",
                "$value": "#625B71"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#E8DEF8"
            },
            "on-container": {
                "$type": "color",
                "$value": "#1D192B"
            }
        },
        "tertiary": {
            "default": {
                "$type": "color",
                "$value": "#7D5260"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#FFD8E4"
            },
            "on-container": {
                "$type": "color",
                "$value": "#31111D"
            }
        },
        "error": {
            "default": {
                "$type": "color",
                "$value": "#B3261E"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#F9DEDC"
            },
            "on-container": {
                "$type": "color",
                "$value": "#410E0B"
            }
        },
        "background": {
            "default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "on-default": {
                "$type": "color",
                "$value": "#1C1B1F"
            }
        },
        "surface": {
            "default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "on-default": {
                "$type": "color",
                "$value": "#1C1B1F"
            },
            "variant": {
                "$type": "color",
                "$value": "#E7E0EC"
            },
            "on-variant": {
                "$type": "color",
                "$value": "#49454F"
            }
        },
        "outline": {
            "default": {
                "$type": "color",
                "$value": "#79747E"
            }
        },
        "success": {
            "default": {
                "$type": "color",
                "$value": "#388E3C"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#C8E6C9"
            },
            "on-container": {
                "$type": "color",
                "$value": "#1B5E20"
            }
        },
        "warning": {
            "default": {
                "$type": "color",
                "$value": "#F57C00"
            },
            "on-default": {
                "$type": "color",
                "$value": "#FFFFFF"
            },
            "container": {
                "$type": "color",
                "$value": "#FFE0B2"
            },
            "on-container": {
                "$type": "color",
                "$value": "#E65100"
            }
        }
    },
    "border": {
        "thin": {
            "{color}": {
                "$type": "border",
                "$value": {
                    "width": "{border-width.1}",
                    "style": "{border-style.solid}",
                    "color": "{$value}"
                }
            }
        },
        "medium": {
            "{color}": {
                "$type": "border",
                "$value": {
                    "width": "{border-width.2}",
                    "style": "{border-style.solid}",
                    "color": "{$value}"
                }
            }
        },
        "bold": {
            "{color}": {
                "$type": "border",
                "$value": {
                    "width": "{border-width.3}",
                    "style": "{border-style.solid}",
                    "color": "{$value}"
                }
            }
        },
        "black": {
            "{color}": {
                "$type": "border",
                "$value": {
                    "width": "{border-width.4}",
                    "style": "{border-style.solid}",
                    "color": "{$value}"
                }
            }
        }
    },
    "border-radius": {
        "extra-large": {
            "$type": "string",
            "$value": "{border-radius.10}"
        },
        "large": {
            "$type": "string",
            "$value": "{border-radius.8}"
        },
        "medium": {
            "$type": "string",
            "$value": "{border-radius.6}"
        },
        "small": {
            "$type": "string",
            "$value": "{border-radius.4}"
        },
        "extra-small": {
            "$type": "string",
            "$value": "{border-radius.2}"
        },
        "max": {
            "$type": "string",
            "$value": "{border-radius.full}"
        }
    },
    "spacing": {
        "extra-large": {
            "$type": "string",
            "$value": "{spacing.9}"
        },
        "large": {
            "$type": "string",
            "$value": "{spacing.7}"
        },
        "medium": {
            "$type": "string",
            "$value": "{spacing.5}"
        },
        "small": {
            "$type": "string",
            "$value": "{spacing.3}"
        },
        "extra-small": {
            "$type": "string",
            "$value": "{spacing.1}"
        }
    },
    "shadow": {
        "top-left": {
            "large": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.6}",
                    "offsetY": "{box-shadow.offset-y.6}",
                    "blur": "{box-shadow.blur-radius.10}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "medium": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.4}",
                    "offsetY": "{box-shadow.offset-y.4}",
                    "blur": "{box-shadow.blur-radius.7}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "small": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.1}",
                    "offsetY": "{box-shadow.offset-y.1}",
                    "blur": "{box-shadow.blur-radius.5}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            }
        },
        "top-right": {
            "large": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.6}",
                    "offsetY": "{box-shadow.offset-y.6}",
                    "blur": "{box-shadow.blur-radius.10}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "medium": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.4}",
                    "offsetY": "{box-shadow.offset-y.4}",
                    "blur": "{box-shadow.blur-radius.7}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "small": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.1}",
                    "offsetY": "{box-shadow.offset-y.1}",
                    "blur": "{box-shadow.blur-radius.5}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            }
        },
        "bottom-left": {
            "large": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.6}",
                    "offsetY": "{box-shadow.offset-y.6}",
                    "blur": "{box-shadow.blur-radius.10}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "medium": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.4}",
                    "offsetY": "{box-shadow.offset-y.4}",
                    "blur": "{box-shadow.blur-radius.7}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "small": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.1}",
                    "offsetY": "{box-shadow.offset-y.1}",
                    "blur": "{box-shadow.blur-radius.5}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            }
        },
        "bottom-right": {
            "large": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.6}",
                    "offsetY": "{box-shadow.offset-y.6}",
                    "blur": "{box-shadow.blur-radius.10}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "medium": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.4}",
                    "offsetY": "{box-shadow.offset-y.4}",
                    "blur": "{box-shadow.blur-radius.7}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            },
            "small": {
                "$type": "shadow",
                "$value": {
                    "offsetX": "{box-shadow.offset-x.1}",
                    "offsetY": "{box-shadow.offset-y.1}",
                    "blur": "{box-shadow.blur-radius.5}",
                    "spread": "{box-shadow.spread-radius.1}",
                    "color": "{color.primary.on-default}"
                }
            }
        }
    }
}
