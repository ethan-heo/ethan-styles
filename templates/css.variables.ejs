[data-theme="<%= topLevelMeta.$theme %>"] {
    <% tokens.forEach((token) => { %>
        <% if (!isTokenObj(token.value)) return %>
        <% const variable = getCSSVariable(token, '-') %>
        <% if (Array.isArray(variable.value)) { %>
            <% variable.value.forEach(([key, value]) => { %>
                --<%= variable.key %>-<%= key %>: <%= value %>;
            <% }) %>
        <% } else { %>
            --<%= variable.key %>: <%= variable.value %>;
        <% } %>
    <% }) %>
}