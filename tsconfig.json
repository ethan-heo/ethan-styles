{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true
  },
  "include": ["scripts/**/*"]
}
