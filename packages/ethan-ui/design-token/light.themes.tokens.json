{
	"grid": {
		"mobile-portrait": {
			"columns": {
				"$type": "number",
				"$value": "4"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.1}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "{spacing.4}"
			}
		},
		"mobile-landscape": {
			"columns": {
				"$type": "number",
				"$value": "6"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.2}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "{spacing.6}"
			}
		},
		"tablet-portrait": {
			"columns": {
				"$type": "number",
				"$value": "8"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.3}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "{spacing.8}"
			}
		},
		"tablet-landscape": {
			"columns": {
				"$type": "number",
				"$value": "12"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.4}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "{spacing.10}"
			}
		},
		"desktop": {
			"columns": {
				"$type": "number",
				"$value": "12"
			},
			"gutter": {
				"$type": "dimension",
				"$value": "{spacing.5}"
			},
			"margin": {
				"$type": "dimension",
				"$value": "auto"
			}
		}
	},
	"responsive": {
		"mobile-portrait": {
			"max": {
				"$type": "dimension",
				"$value": "{breakpoint.mobile-portrait.max}"
			}
		},
		"mobile-landscape": {
			"min": {
				"$type": "dimension",
				"$value": "{breakpoint.mobile-landscape.min}"
			},
			"max": {
				"$type": "dimension",
				"$value": "{breakpoint.mobile-landscape.max}"
			}
		},
		"tablet-portrait": {
			"min": {
				"$type": "dimension",
				"$value": "{breakpoint.tablet-portrait.min}"
			},
			"max": {
				"$type": "dimension",
				"$value": "{breakpoint.tablet-portrait.max}"
			}
		},
		"tablet-landscape": {
			"min": {
				"$type": "dimension",
				"$value": "{breakpoint.tablet-landscape.min}"
			},
			"max": {
				"$type": "dimension",
				"$value": "{breakpoint.tablet-landscape.max}"
			}
		},
		"desktop": {
			"min": {
				"$type": "dimension",
				"$value": "{breakpoint.desktop.min}"
			}
		}
	},
	"font-size": {
		"extra-large": {
			"$type": "dimension",
			"$value": "{font-size.12}"
		},
		"large": {
			"$type": "dimension",
			"$value": "{font-size.8}"
		},
		"medium": {
			"$type": "dimension",
			"$value": "{font-size.4}"
		},
		"small": {
			"$type": "dimension",
			"$value": "{font-size.3}"
		},
		"extra-small": {
			"$type": "dimension",
			"$value": "{font-size.1}"
		}
	},
	"font-weight": {
		"thin": {
			"$type": "number",
			"$value": "100"
		},
		"extra-light": {
			"$type": "number",
			"$value": "200"
		},
		"light": {
			"$type": "number",
			"$value": "300"
		},
		"normal": {
			"$type": "number",
			"$value": "400"
		},
		"medium": {
			"$type": "number",
			"$value": "500"
		},
		"semi-bold": {
			"$type": "number",
			"$value": "600"
		},
		"bold": {
			"$type": "number",
			"$value": "700"
		},
		"extra-bold": {
			"$type": "number",
			"$value": "800"
		},
		"black": {
			"$type": "number",
			"$value": "900"
		}
	},
	"line-height": {
		"extra-large": {
			"$type": "number",
			"$value": "{line-height.12}"
		},
		"large": {
			"$type": "number",
			"$value": "{line-height.8}"
		},
		"medium": {
			"$type": "number",
			"$value": "{line-height.4}"
		},
		"small": {
			"$type": "number",
			"$value": "{line-height.3}"
		},
		"extra-small": {
			"$type": "number",
			"$value": "{line-height.1}"
		}
	},
	"color": {
		"primary": {
			"$type": "color",
			"$value": "{color.orange.1}"
		},
		"secondary": {
			"$type": "color",
			"$value": "{color.white.1}"
		},
		"accent": {
			"$type": "color",
			"$value": "{color.orange.3}"
		},
		"success": {
			"$type": "color",
			"$value": "{color.light-green.5}"
		},
		"warning": {
			"$type": "color",
			"$value": "{color.golden.1}"
		},
		"error": {
			"$type": "color",
			"$value": "{color.red.3}"
		},
		"text": {
			"$type": "color",
			"$value": "{color.dark.3}"
		},
		"background": {
			"$type": "color",
			"$value": "{color.white.1}"
		}
	},
	"border": {
		"{border-style}": {
			"thin": {
				"$type": "string",
				"$value": "{border-width.1} {$value}"
			},
			"medium": {
				"$type": "string",
				"$value": "{border-width.2} {$value}"
			},
			"bold": {
				"$type": "string",
				"$value": "{border-width.3} {$value}"
			},
			"black": {
				"$type": "string",
				"$value": "{border-width.4} {$value}"
			}
		}
	},
	"border-radius": {
		"extra-large": {
			"$type": "number",
			"$value": "{border-radius.10}"
		},
		"large": {
			"$type": "number",
			"$value": "{border-radius.8}"
		},
		"medium": {
			"$type": "number",
			"$value": "{border-radius.6}"
		},
		"small": {
			"$type": "number",
			"$value": "{border-radius.4}"
		},
		"extra-small": {
			"$type": "number",
			"$value": "{border-radius.2}"
		},
		"full": {
			"$type": "number",
			"$value": "{border-radius.full}"
		}
	},
	"spacing": {
		"extra-large": {
			"$type": "number",
			"$value": "{spacing.9}"
		},
		"large": {
			"$type": "number",
			"$value": "{spacing.7}"
		},
		"medium": {
			"$type": "number",
			"$value": "{spacing.5}"
		},
		"small": {
			"$type": "number",
			"$value": "{spacing.3}"
		},
		"extra-small": {
			"$type": "number",
			"$value": "{spacing.1}"
		}
	},
	"shadow": {
		"top-left": {
			"large": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.6} -{box-shadow.offset-y.6} {box-shadow.blur-radius.10} {box-shadow.spread-radius.1}"
			},
			"medium": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.4} -{box-shadow.offset-y.4} {box-shadow.blur-radius.7} {box-shadow.spread-radius.1}"
			},
			"small": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.1} -{box-shadow.offset-y.1} {box-shadow.blur-radius.5} {box-shadow.spread-radius.1}"
			}
		},
		"top-right": {
			"large": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.6} -{box-shadow.offset-y.6} {box-shadow.blur-radius.10} {box-shadow.spread-radius.1}"
			},
			"medium": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.4} -{box-shadow.offset-y.4} {box-shadow.blur-radius.7} {box-shadow.spread-radius.1}"
			},
			"small": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.1} -{box-shadow.offset-y.1} {box-shadow.blur-radius.5} {box-shadow.spread-radius.1}"
			}
		},
		"bottom-left": {
			"large": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.6} {box-shadow.offset-y.6} {box-shadow.blur-radius.10} {box-shadow.spread-radius.1}"
			},
			"medium": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.4} {box-shadow.offset-y.4} {box-shadow.blur-radius.7} {box-shadow.spread-radius.1}"
			},
			"small": {
				"$type": "string",
				"$value": "-{box-shadow.offset-x.1} {box-shadow.offset-y.1} {box-shadow.blur-radius.5} {box-shadow.spread-radius.1}"
			}
		},
		"bottom-right": {
			"large": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.6} {box-shadow.offset-y.6} {box-shadow.blur-radius.10} {box-shadow.spread-radius.1}"
			},
			"medium": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.4} {box-shadow.offset-y.4} {box-shadow.blur-radius.7} {box-shadow.spread-radius.1}"
			},
			"small": {
				"$type": "string",
				"$value": "{box-shadow.offset-x.1} {box-shadow.offset-y.1} {box-shadow.blur-radius.5} {box-shadow.spread-radius.1}"
			}
		}
	},
	"font-family": {
		"$type": "string",
		"$value": "{font-family.default}"
	}
}
